{
  "name": "@oracle/oraclejet-preact",
  "version": "13.1.0",
  "description": "Oracle JET preact based components",
  "scripts": {
    "dev": "cross-env STORIES=dev SB_PORT=6006 yarn storybook",
    "docs": "cross-env STORIES=docs SB_PORT=6007 yarn storybook",
    "build-dev": "cross-env STORIES=dev yarn build-storybook",
    "build-docs": "cross-env STORIES=docs yarn build-storybook",
    "build": "yarn clean && yarn rollup && yarn build:index && yarn copy:pkg && yarn copy:images",
    "build:index": "rollup -c scripts/rollup.index.js && rimraf 'dist/types/**/index.js'",
    "copy:pkg": "copyfiles package.json README.md THIRDPARTYLICENSE.txt dist",
    "copy:images": "copyfiles -f src/**/images/** dist/amd/images && copyfiles -f src/**/images/** dist/es/images && copyfiles -f src/**/images/** dist/sb-static/images",
    "build:watch": "rollup -c scripts/rollup.config.js --watch",
    "build:l10n": "yarn node scripts/build-l10n.js --rootDir=src/resources/icu --bundleName=bundle.json --locale=en-US --outDir=src/resources/nls",
    "build:icons-base": "rimraf src/UNSAFE_Icons && svgr --config-file scripts/svgr.config.js --svgo-config scripts/svgo.config.js --out-dir src/UNSAFE_Icons -- src/resources/icons",
    "build:icons-themed": "rimraf src/UNSAFE_ThemedIcons && node scripts/buildThemedIcons.js",
    "build:icons": "node scripts/fetchRedwoodImage.js && yarn build:icons-base && yarn build:icons-themed",
    "rollup": "rollup -c scripts/rollup.config.js",
    "clean": "rimraf dist test_results",
    "package": "cd dist && npm pack",
    "test": "jest -c test/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:browser": "yarn test:browser:convert && yarn run karma start test/karma.conf.js",
    "test:browser:convert": "node test/convert-testing-libs.js",
    "test:browser:watch": "yarn test:browser --singleRun=false",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand -c scripts/jest.config.js",
    "storybook": "cross-env-shell \"start-storybook -p $SB_PORT -s dist/sb-static\"",
    "storybook-docs": "start-storybook --docs --no-manager-cache -p 6006 -s dist/sb-static",
    "build-storybook": "build-storybook -o dist/storybook-static -s dist/sb-static",
    "build-storybook-docs": " && build-storybook --docs -s dist/sb-static",
    "type-check": "tsc -p . --noEmit"
  },
  "author": "",
  "license": "UPL-1.0",
  "keywords": [
    "oraclejet"
  ],
  "devDependencies": {
    "@babel/cli": "~7.16.7",
    "@babel/core": "~7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "~7.16.7",
    "@babel/plugin-syntax-flow": "~7.16.7",
    "@babel/plugin-transform-spread": "~7.16.7",
    "@babel/preset-flow": "~7.16.7",
    "@linaria/babel-preset": "3.0.0-beta.15",
    "@linaria/core": "3.0.0-beta.4",
    "@linaria/rollup": "3.0.0-beta.7",
    "@linaria/shaker": "3.0.0-beta.7",
    "@linaria/webpack-loader": "3.0.0-beta.13",
    "@linaria/webpack4-loader": "3.0.0-beta.13",
    "@oracle/oraclejet-cssprops": "workspace:^",
    "@oracle/oraclejet-icu-l10n": "workspace:^",
    "@oracle/oraclejet-storybook-addons-reading": "workspace:^",
    "@oracle/oraclejet-storybook-addons-theme": "workspace:^",
    "@oracle/oraclejet-storybook-patch": "workspace:^",
    "@rollup/plugin-alias": "~3.1.9",
    "@rollup/plugin-babel": "~5.3.1",
    "@rollup/plugin-commonjs": "~20.0.0",
    "@rollup/plugin-dynamic-import-vars": "~1.4.3",
    "@rollup/plugin-json": "~4.1.0",
    "@rollup/plugin-node-resolve": "~13.1.3",
    "@rollup/plugin-replace": "~3.1.0",
    "@rollup/plugin-typescript": "~8.3.1",
    "@rollup/plugin-virtual": "~2.1.0",
    "@storybook/addon-actions": "6.5.8",
    "@storybook/addon-docs": "6.5.8",
    "@storybook/addon-essentials": "6.5.8",
    "@storybook/addon-links": "6.5.8",
    "@storybook/mdx2-csf": "0.0.3",
    "@storybook/preact": "6.5.8",
    "@svgr/babel-plugin-add-jsx-attribute": "~6.0.0",
    "@svgr/babel-plugin-remove-jsx-attribute": "~6.0.0",
    "@svgr/cli": "~6.2.1",
    "@testing-library/dom": "~8.11.3",
    "@testing-library/preact": "~2.0.1",
    "@testing-library/preact-hooks": "~1.1.0",
    "@testing-library/user-event": "~13.5.0",
    "@types/chai": "~4.3.0",
    "@types/jest": "~27.4.1",
    "@types/ramda": "~0.27.66",
    "@types/sinon": "~10.0.11",
    "babel-loader": "~8.2.2",
    "babel-plugin-react-docgen": "~4.2.1",
    "chai": "~4.3.6",
    "copyfiles": "~2.4.1",
    "core-js": "~3.21.1",
    "cross-env": "~7.0.3",
    "css-loader": "~3.6.0",
    "fs-extra": "^10.1.0",
    "https-proxy-agent": "~5.0.0",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~27.5.1",
    "jest-html-reporters": "3.0.6",
    "jest-junit": "~13.2.0",
    "jsdom": "~19.0.0",
    "jsdom-global": "~3.0.2",
    "karma": "~5.2.3",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-html-reporter": "0.2.7",
    "karma-junit-reporter": "~2.0.1",
    "karma-mocha": "~1.3.0",
    "karma-requirejs": "~1.1.0",
    "linaria": "~2.3.1",
    "mocha": "~6.2.3",
    "preact": "10.7.2",
    "preact-render-to-string": "~5.1.20",
    "react-docgen-typescript-loader": "~3.7.2",
    "require-css": "~0.1.10",
    "requirejs": "2.3.6",
    "rimraf": "~3.0.2",
    "rollup": "~2.70.0",
    "rollup-plugin-copy": "~3.4.0",
    "rollup-plugin-node-globals": "~1.4.0",
    "rollup-plugin-terser": "~7.0.2",
    "rollup-plugin-typescript": "~1.0.1",
    "sinon": "~7.5.0",
    "storybook-addon-performance": "~0.16.1",
    "ts-jest": "~27.1.3",
    "typescript": "4.6.4",
    "webpack": "~4.46.0"
  },
  "dependencies": {
    "@floating-ui/dom": "~0.5.2",
    "ramda": "~0.27.2"
  },
  "files": [
    "amd",
    "cjs",
    "es",
    "types",
    "!**/*.spec.*",
    "THIRDPARTYLICENSE.txt"
  ],
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "peerDependencies": {
    "preact": "10.7.2"
  }
}
